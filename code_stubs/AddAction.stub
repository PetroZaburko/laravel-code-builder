<?php

declare(strict_types=1);

namespace {namespace};

use {model_namespace};

final class {name}
{
    public function handle(array $data): {model_name}
    {
        ${entity_singular} = new {model_name}();

        ${entity_singular}->fill($data);

        ${entity_singular}->save();

        ${entity_singular}->refresh();

        return ${entity_singular};
    }
}