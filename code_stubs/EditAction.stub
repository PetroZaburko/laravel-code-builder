<?php

declare(strict_types=1);

namespace {namespace};

use {model_namespace};

final class {name}
{
    public function handle(int $id, array $data): {model_name}
    {
        $model = {model_name}::query()->where('id', $id)->first();
        
        $model->fill($data);

        $model->save();

        $model->refresh();

        return $model;
    }
}